<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="net.jessechen.accessibleyoutube" android:versionCode="1"
	android:versionName="1.0">
	
	<uses-permission
        android:name="android.permission.INTERNET" />
	
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity android:name=".Main" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

	
<activity android:clearTaskOnLaunch="true" android:name=".QueryResults"></activity>
<activity android:name=".Result"></activity>

<activity android:name=".ChannelResults"></activity>

</application>


</manifest> 
